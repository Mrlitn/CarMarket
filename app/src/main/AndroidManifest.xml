<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="main.java.com.carmarket">

    <!--权限-->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.market.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.market.activity.TwoActivity"
            android:launchMode="singleInstance"></activity>

        <activity android:name="com.market.activity.ThreeActivity"></activity>
        <activity android:name="com.market.activity.SearchActivity"></activity>

        <activity android:name="com.market.activity.FourActivity"></activity>

        <activity android:name="com.market.views.MetionActivity"></activity>

        <service android:name="com.market.activity.oneService"></service>

        <receiver android:name="com.market.activity.MyBroacaset">
            <intent-filter android:priority="1000">
                <action android:name="com.litn.good" />
            </intent-filter>
        </receiver>

    </application>

</manifest>